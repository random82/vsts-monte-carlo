  "name": "vsts-monte-carlo",
  "version": "0.0.1",
  "description": "Monte Carlo forecast plugin for VSTS",
  "scripts": {
    "test": "karma start",
    "build:dev": "rimraf dist *.vsix && webpack --config config/webpack.dev.js --progress --profile",
    "build:prod": "rimraf dist *.vsix && webpack --config config/webpack.prod.js --progress --profile",
    "postinstall": "typings install",
    "package": "tfx extension create --manifest-globs vss-extension.json --rev-version"
  },
